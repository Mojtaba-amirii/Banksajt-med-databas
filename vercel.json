{
  "projects": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node",
      "config": {
        "functions": {
          "api/**/*.js": {
            "runtime": "nodejs18.x"
          }
        }
      }
    }
  ]
}
